$('<%=@div%>').html("<%= escape_javascript(render :partial => 'saved_items/description', :locals=>{:edit_id => nil, :saved_item=>@saved_item})%>");
<% unless @message.blank? %>
	$('.flash_messages').append("<%= escape_javascript(render :partial => 'shared/flash_success_message') %>");
<% end %>
<% unless @link_text.blank? %>
	$("#item_note_link_<%=@saved_item.id%>").html("<%=@link_text %>");
<% end %>
