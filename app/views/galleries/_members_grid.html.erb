<div class="col-xs-12 collection-members">

  <div class="row collection-members-grid-header">
      <div class="col-xs-12 col-sm-8">
        <h5>Other items in "<%=@gallery.title%>" <span class="text-muted">(<%=@saved_items.size%> of <%=@gallery.saved_items(current_user).size%>)</span></h5>
      </div>
          <div class="col-xs-12 col-sm-3 col-sm-offset-1 controls">
           <%=link_to t('revs.user_galleries.view'), gallery_path(params[:gallery_id],extract_paging_params(params))%>
      </div>
  </div>

  <div class="row">
    <div class="col-xs-12">
      <%- @saved_items.each do |saved_item| -%>
        <div class="item">
          <%= link_to(image_tag(saved_item.image), item_path(saved_item.solr_document,extract_paging_params(params).merge({:gallery_id=>params[:gallery_id]}))) %>
        </div>
      <%- end -%>
    </div>
  </div>

</div>
